TREEVIEW OBJECT PROPERTIES NAME ( "Nullebene" )
TREEVIEW OBJECT SELECT NONE
TREEVIEW PRIMITIVE SELECT MEASURED
FEATURE NOMINAL_MEASURED SWAP_PRIMITIVES ( )

TREEVIEW OBJECT SELECT NONE
TREEVIEW OBJECT SELECT ( "Nullpunkt" )
TREEVIEW OBJECT SELECT ( "Stiftaufnahme" )
FEATURE PRIMITIVE LINE FROM_CENTER_POINTS ( , )
TREEVIEW OBJECT PROPERTIES NAME ( "Wector" )

TREEVIEW OBJECT SELECT NONE
TREEVIEW OBJECT SELECT ( "Nullebene" )
MACRO PAUSE ( "Ausrichtung", "Nullebene tasten, bitte",)
FEATURE PRIMITIVE PLANE PROBE ( , "Nullebene", )

TREEVIEW OBJECT SELECT NONE
TREEVIEW OBJECT SELECT ( "Nullpunkt" )
TREEVIEW FEATURE PROPERTIES MEASUREMENT_METHOD ( "Probe", )
TREEVIEW FEATURE CURVE_BASED PROPERTIES PROBE USE_CONSTRAINING_PLANE ( "On", )
TREEVIEW FEATURE CURVE_BASED PROPERTIES PROBE CONSTRAINING_PLANE METHOD ( "Use Plane Feature", )
TREEVIEW FEATURE CURVE_BASED PROPERTIES PROBE CONSTRAINING_PLANE PLANE_FEATURE ( "Nullebene", )
MACRO PAUSE ( "Ausrichtung", "Nullpunkt tasten, bitte",)
FEATURE PRIMITIVE CIRCLE PROBE ( , "Nullpunkt", )

TREEVIEW OBJECT SELECT NONE
TREEVIEW OBJECT SELECT ( "Stiftaufnahme" )
TREEVIEW FEATURE PROPERTIES MEASUREMENT_METHOD ( "Probe", )
TREEVIEW FEATURE CURVE_BASED PROPERTIES PROBE USE_CONSTRAINING_PLANE ( "On", )
TREEVIEW FEATURE CURVE_BASED PROPERTIES PROBE CONSTRAINING_PLANE METHOD ( "Use Plane Feature", )
TREEVIEW FEATURE CURVE_BASED PROPERTIES PROBE CONSTRAINING_PLANE PLANE_FEATURE ( "Nullebene", )
MACRO PAUSE ( "Ausrichtung", "Stiftaufnahme tasten, bitte",)
FEATURE PRIMITIVE CIRCLE PROBE ( , "Stiftaufnahme", )


TREEVIEW OBJECT SELECT NONE
TREEVIEW OBJECT SELECT ( "Nullpunkt" )
TREEVIEW OBJECT SELECT ( "Stiftaufnahme" )
TREEVIEW OBJECT SELECT ( "Nullebene" )
TREEVIEW OBJECT SELECT ( "Wector" )
TREEVIEW FEATURE GROUP ( )
TREEVIEW OBJECT_GROUP PROPERTIES NAME ( "Ausrichtung",  )

ALIGN PLANE_AXIS_CENTER_POINT ( "Nullebene -meas-", "Nullebene -nom-", "Wector -meas-", "Wector -nom-", "Nullpunkt -meas-", "Nullpunkt -nom-", "plane, axis, center point 1" )

TREEVIEW OBJECT SELECT NONE
TREEVIEW OBJECT_GROUP SELECT ( "Ausrichtung" )
VIEW POSE CENTER_ON_OBJECTS ( )
