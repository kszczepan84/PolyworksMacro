DECLARE vNull
DECLARE vStift
DECLARE vNullebene

TREEVIEW OBJECT SELECT NONE
MACRO PAUSE ( "Ausrichtung", "Wählen Sie Nullpunkt und recht Maus tasten, bitte",)
FEATURE PRIMITIVE CIRCLE PICK_ON_NURBS
TREEVIEW OBJECT PROPERTIES NAME ( "Nullpunkt" )
TREEVIEW OBJECT SELECT NONE
TREEVIEW PRIMITIVE SELECT MEASURED
FEATURE NOMINAL_MEASURED SWAP_PRIMITIVES ( )

TREEVIEW OBJECT SELECT NONE
MACRO PAUSE ( "Ausrichtung", "Wählen Sie Stiftaufnahme und recht Maus tasten, bitte",)
FEATURE PRIMITIVE CIRCLE PICK_ON_NURBS
TREEVIEW OBJECT PROPERTIES NAME ( "Stiftaufnahme" )
TREEVIEW OBJECT SELECT NONE
TREEVIEW PRIMITIVE SELECT MEASURED
FEATURE NOMINAL_MEASURED SWAP_PRIMITIVES ( )


TREEVIEW OBJECT SELECT NONE
MACRO PAUSE ( "Ausrichtung", "Wählen Sie Nullebene und recht Maus tasten, bitte",)
