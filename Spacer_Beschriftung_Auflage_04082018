DECLARE value_X
DECLARE value_Y
DECLARE value_Z
MACRO INPUT STRING ( value_X,"Spacer", "Einlegen Richtung X Spacer Maﬂ",  )
MACRO INPUT STRING ( value_Y,"Spacer", "Einlegen Richtung Y Spacer Maﬂ",  )
MACRO INPUT STRING ( value_Z,"Spacer", "Einlegen Richtung Z Spacer Maﬂ",  )
VIEW ANNOTATION TEMPLATE ASSIGN ( "FEE_Auflage+Spacer", "User", )
MEASURE CONTROL NOTE ( "Surface Point X", "   ${value_X}", )
MEASURE CONTROL NOTE ( "Surface Point Y", "   ${value_Y}", )
MEASURE CONTROL NOTE ( "Surface Point Z", "   ${value_Z}", )